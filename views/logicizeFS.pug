extends pagelayout

block extrahead
	link(href="https://fonts.googleapis.com/css?family=Abril+Fatface|Titillium+Web:400" rel="stylesheet")

block content
	-console.log(ranking)
	main.container.logicizeFS.p-x-1(id = logicise)
		.logicizeTitle.m-b-2
			h2.m-b-0 #{info.title}
			nav.navbar-light.navbar.navbar-full.p-x-0#logicizeNav.subNav
				ul.nav.navbar-nav
					li.nav-item
						span.navbar-text.text-muted.hidden-md-down #{info.shortDescription}
					li.nav-item.pull-xs-right
						a.nav-link.p-y-0#leaderboardButt(href = "#") Leaderboard
					li.nav-item.pull-xs-right
						a.nav-link.p-y-0#tutorial(href = "#") Tutorial
					li.nav-item.pull-xs-right
						span.navbar-text Passing Score: &nbsp;
						span#passingscore
					li.nav-item.pull-xs-right
						span.navbar-text Score : &nbsp;
						span#score 0
		.row.text-xs-center
			include vennSyl
	.modal.fade(tabindex="-1" role="dialog" aria-labelledby="messageLabel" aria-hidden="true")#messageModal
		.modal-dialog.modal-lg(role="document")
			.modal-content
				.modal-header
					button.close(type="button" data-dismiss="modal" aria-label="Close")
						span(aria-hidden="true") &times;
					h4.modal-title#messageLabel Message Title
				.modal-body
	.sidenav#leaderboard
		a.closebtn#leaderclose(href = "javascript:void(0)") &times;
		h4.text-xs-center Leaderboard
		#board






block extrafooter
	link(rel="stylesheet", type="text/css", href="../css/logicize.css")
	script(type="text/x-mathjax-config").
		MathJax.Hub.Config({
		tex2jax: {
			inlineMath: [ ['$','$'], ['\\(','\\)'] ]
		}}
		);
	script(type="text/javascript"  src="https://cdn.mathjax.org/mathjax/latest/MathJax.js?config=TeX-MML-AM_HTMLorMML")
	script(src="https://d3js.org/d3.v4.min.js")
	script(src="https://d3js.org/d3-scale-chromatic.v1.min.js")
	script(src="../javascripts/mods/notify.js")
	script(src="../javascripts/"+logicise+".js")
	script(src="../javascripts/autohidenav.js")
	script(src="../javascripts/leaderboard.js")
	link(rel="stylesheet", type="text/css", href="/css/leaderboard.css")
